{
  "name": "cms/block-bundle",
  "type": "symfony-bundle",
  "description": "Manage page's content with block",
  "keywords": [
    "block",
    "cms",
    "page"
  ],
  "homepage": "https://github.com/breizhman/BlockBundle",
